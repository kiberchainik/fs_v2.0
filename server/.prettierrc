{
  "singleQuote": true,
  "trailingComma": "none",
  "tabWidht": 4,
  "semi": false,
  "useTabs": true,
  "arrowParens": "avoid",
  "jsxSingleQuote": true,
  "importOrderSeparetion": true,
  "importOrderSortSpecifiers": true,
  "importOrderCaseInsensitive": true,
  "importOrderParserPlugins": [
    "classProperties",
    "decorators-legacy",
    "typescript"
  ],
  "importOrder":["<THIRD_PARTY_MODULES>", "^@/(.*)$", "^../(.*)", "^./(.*)"],
  "plugins": ["@trivago/prettier-plugin-sort-imports"]
}
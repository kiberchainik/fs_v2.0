{
    "trailingComma": "none",
    "tabWidth": 4,
    "useTabs": true,
    "semi": false,
    "jsxSingleQuote": true,
    "singleQuote": true,
    "arrowParens": "avoid",
    "importOrder": [
        "<THIRD_PARTY_MODULES>",
        "^@/app/(.*)$",
        "^@/features/(.*)$",
        "^@/shared/(.*)$",
        "^@../(.*)$",
        "^@./(.*)$"
    ],
    "importOrderSeparation": true,
    "importOrderSortSeparation": true,
    "plugins": ["@trivago/prettier-plugin-sort-imports", "prettier-plugin-tailwindcss"]
}